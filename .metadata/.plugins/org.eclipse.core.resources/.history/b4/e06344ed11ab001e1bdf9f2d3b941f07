public class SemanticAnalyzer extends VisitorAdaptor {
	
	Logger log =  Logger.getLogger(getClass());
	

	public void visit(ProgramName progName)
	{
		Tab.insert(Obj.Prog, progName.getProgName(), Tab.noType);
		//Tab.openScope();
	}
}